{
  "name": "react-augmentation",
  "version": "1.2.1",
  "description": "DOM augmentation operators to simplify UI code and promote data-driven DOM hierarchy",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "keywords": [
    "augment",
    "augmentation",
    "augmented",
    "dialog",
    "inlet",
